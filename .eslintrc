{
    "parser": "babel-eslint",
    "rules": {
        "indent": [
            2,
            4 
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "eol-last": 1,
        "dot-notation": 0,
        "no-console":0,
        "no-undef": 2,
        "no-unused-vars": 2
    },
    "env": {
        "node": true
    },
    "globals": {
        "angular": true,
        "_": true
    }
}
